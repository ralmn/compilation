int main(){

    int i;
    for(i=0; i<3; i=i+1){
        println(i);
    }

    for(i=0; i>3; i=i+1){
        println(i);
    }

    for(i=0; i<5; i=i+2){
        println(i);
    }

    for(i=0; !i; i=i+1){
        println(i);
    }

    for(i=0; i<4; i=i+1){
        println(i);
        i=i+1;
    }

    for(i=0; i<=2; i=i+1)
        println(i);
}